{
  "build": {
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "Student Management",
  "mainBinaryName": "Student Management",
  "version": "1.2.0",
  "identifier": "com.rayat.dev",
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERGNTg0QjJFMjkzM0FEMkUKUldRdXJUTXBMa3RZMzZiUnNWWkFWUmNlYy82M1pGMXJEY1JZV1V1bGJoNXRxeUt3UkZIWUhUWmoK",
      "endpoints": [
        "https://student-management-server-pi.vercel.app/{{target}}/{{arch}}/{{current_version}}"
      ]
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Student Management!",
        "label": "main",
        "width": 1024,
        "height": 720,
        "visible": false,
        "center": true,
        "useHttpsScheme": true
      },
      {
        "width": 800,
        "height": 600,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "center": true,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
